  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mapkeeper.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/mapkeeper/issues"
  },
  "homepage": "https://github.com/yourusername/mapkeeper#readme",
